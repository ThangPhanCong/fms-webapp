pipeline {
    agent {
        label 'master'
    }

    stages {
      stage('Install Dependencies') {
          steps {
              sh 'npm i'
          }
      }

      stage('Build') {
          steps {
              sh 'npm run build-staging'
          }
      }

      stage('Deploy to staging') {
          steps {
			        sh 'echo "deploy ok"'
          }
      }
    }
}
